{
  "expo": {
    "name": "Notifications",
    "slug": "Notifications",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#EFE7DD"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-image.png",
        "backgroundColor": "#EFE7DD"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
        [
            "expo-image-picker",
            {
              "photosPermission": "La aplicación accede a la galería para poder utilizar la foto que quieras como fondo decorativo"
            }
        ]
    ]
  }
}
